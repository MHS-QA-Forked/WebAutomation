<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Ebay Automation Script " parallel="false">

    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Mojave"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="73.0.3683.68"/>
    <parameter name="url" value="https://www.ebay.com/"/>

    <test name = "Ebay Test">
        <classes>
            <class name="ebayTests.HomeUtilitiesTests"></class>
            <class name="ebayTests.PurchseTest"></class>
            <class name="ebayTests.signInTest"></class>
        </classes>
    </test>

</suite>